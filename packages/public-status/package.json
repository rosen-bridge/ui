{
  "name": "@rosen-ui/public-status",
  "version": "0.1.0",
  "description": "Core logic package of public status",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "coverage": "npm run test -- --run --coverage",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "test": "NODE_OPTIONS='--import tsx' vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@rosen-bridge/extended-typeorm": "^1.0.1"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

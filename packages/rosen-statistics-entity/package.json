{
  "name": "@rosen-ui/rosen-statistics-entity",
  "version": "0.1.0",
  "private": true,
  "description": "Rosen statistics entity and migrations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "release": "npm run test -- --run && npm run clean && npm run build && npm publish --access public",
    "test": "NODE_OPTIONS='--import tsx' vitest",
    "coverage": "npm run test -- --coverage",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.1.4",
    "tsx": "^4.19.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@rosen-bridge/abstract-logger": "^3.0.1",
    "@rosen-bridge/extended-typeorm": "^1.0.1",
    "@rosen-bridge/token-price-entity": "^0.2.0",
    "@rosen-bridge/watcher-data-extractor": "^12.3.0",
    "@rosen-clients/ergo-explorer": "^2.1.0",
    "@rosen-clients/ergo-node": "^3.1.1",
    "@rosen-ui/asset-calculator": "^2.3.0",
    "ergo-lib-wasm-nodejs": "^0.24.1",
    "@rosen-bridge/json-bigint": "^1.1.0"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

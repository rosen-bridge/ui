{
  "name": "@rosen-bridge/icons",
  "version": "3.1.0",
  "private": "true",
  "description": "A react icon set wrapping Unicons, used in Rosen apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./generate.js && tsc && vite build",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "release": "echo 'this package should not be published due to the license constraints'",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "glob": "^11.0.1",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-svgr": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

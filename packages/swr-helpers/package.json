{
  "name": "@rosen-ui/swr-helpers",
  "version": "0.2.2",
  "private": true,
  "description": "This is a private package utilized within Rosen UI apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "json-bigint": "^1.0.0"
  },
  "devDependencies": {
    "@types/json-bigint": "^1.0.1"
  },
  "peerDependencies": {
    "swr": "^2.2.0"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

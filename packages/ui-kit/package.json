{
  "name": "@rosen-bridge/ui-kit",
  "version": "5.0.0",
  "private": true,
  "description": "ui kit for Rosen projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/components/common/smartSearch/server": "./dist/components/common/smartSearch/server.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --watch",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "release": "npm run build && npm publish --access public",
    "type-check": "tsc"
  },
  "dependencies": {
    "@mui/lab": "^5.0.0-alpha.134",
    "@mui/material": "^5.13.4",
    "@rosen-bridge/icons": "^3.2.0",
    "@rosen-ui/constants": "^1.0.0",
    "@rosen-ui/swr-helpers": "^0.2.2",
    "@rosen-ui/utils": "^1.0.1",
    "embla-carousel-react": "^8.5.2",
    "joi": "^17.11.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "qrcode.react": "^3.1.0",
    "react-hook-form": "^7.45.1",
    "react-sticky-box": "^2.0.5"
  },
  "devDependencies": {
    "@rosen-ui/types": "^0.4.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.1.5"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

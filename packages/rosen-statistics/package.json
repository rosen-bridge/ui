{
  "name": "@rosen-ui/rosen-statistics",
  "version": "0.0.0",
  "description": "Rosen Statistics package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "release": "npm run test -- --run && npm run build && npm publish --access public",
    "test": "NODE_OPTIONS='--import tsx' vitest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.1.4",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}

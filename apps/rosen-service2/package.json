{
  "name": "@rosen-bridge/rosen-service2",
  "version": "0.0.2",
  "description": "",
  "repository": "",
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "coverage": "npm run test -- --run --coverage",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "start": "tsx ./src/index.ts",
    "start:dev": "tsx watch ./src/index.ts",
    "test": "NODE_OPTIONS='--import tsx' vitest",
    "type-check": "tsc --noEmit",
    "typeorm": "NODE_OPTIONS='--import tsx' typeorm",
    "typeorm:generate": "npm run typeorm migration:generate ./src/db/migrations/migration -- -p -d ./src/data-source.ts",
    "typeorm:migrate": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "gen-config-types": "NODE_OPTIONS='--import tsx' npx @rosen-bridge/config generate-ts-types --schema ./config/schema.json --output ./src/types/configs.ts --root-type RosenService2BaseConfig",
    "gen-config-defaults": "NODE_OPTIONS='--import tsx' npx @rosen-bridge/config generate-default --schema ./config/schema.json --output ./config/default.yml --format yaml"
  },
  "dependencies": {
    "@rosen-bridge/address-extractor": "^6.2.2",
    "@rosen-bridge/abstract-logger": "^3.0.1",
    "@rosen-bridge/abstract-observation-extractor": "^0.2.3",
    "@rosen-bridge/abstract-scanner": "^0.2.3",
    "@rosen-bridge/bitcoin-observation-extractor": "^6.4.1",
    "@rosen-bridge/bitcoin-scanner": "^0.2.3",
    "@rosen-bridge/callback-logger": "^1.0.1",
    "@rosen-bridge/cardano-observation-extractor": "^1.1.1",
    "@rosen-bridge/cardano-scanner": "^1.0.1",
    "@rosen-bridge/config": "^1.1.0",
    "@rosen-bridge/discord-notification": "^1.0.0",
    "@rosen-bridge/ergo-observation-extractor": "^0.3.1",
    "@rosen-bridge/ergo-scanner": "^0.1.4",
    "@rosen-bridge/evm-observation-extractor": "^5.4.1",
    "@rosen-bridge/evm-scanner": "^0.1.4",
    "@rosen-bridge/extended-typeorm": "^1.0.1",
    "@rosen-bridge/health-check": "^8.0.0",
    "@rosen-bridge/json-bigint": "^1.1.0",
    "@rosen-bridge/log-level-check": "^3.0.0",
    "@rosen-bridge/scanner-interfaces": "^0.2.1",
    "@rosen-bridge/scanner-sync-check": "^8.1.0",
    "@rosen-bridge/service-manager": "^1.0.1",
    "@rosen-bridge/tokens": "^4.0.1",
    "@rosen-bridge/watcher-data-extractor": "^12.3.0",
    "@rosen-bridge/winston-logger": "^2.0.1",
    "@rosen-clients/ergo-explorer": "^2.1.0",
    "@rosen-ui/asset-calculator": "^2.2.0",
    "@rosen-ui/asset-data-adapter": "0.2.0",
    "@rosen-ui/constants": "^1.0.0",
    "@vercel/kv": "^3.0.0",
    "config": "^3.3.7",
    "ergo-lib-wasm-nodejs": "^0.24.1",
    "ethers": "6.13.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/config": "^0.0.41"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

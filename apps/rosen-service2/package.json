{
  "name": "@rosen-bridge/rosen-service2",
  "version": "0.0.2",
  "description": "",
  "repository": "",
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "coverage": "npm run test -- --coverage",
    "lint": "eslint --fix && npm run prettify",
    "prettify": "prettier --write . --ignore-path ./.gitignore",
    "start": "tsx ./src/index.ts",
    "start:dev": "tsx watch ./src/index.ts",
    "test": "NODE_OPTIONS='--import tsx' vitest",
    "type-check": "tsc --noEmit",
    "typeorm": "NODE_OPTIONS='--import tsx' typeorm",
    "typeorm:generate": "npm run typeorm migration:generate ./src/db/migrations/migration -- -p -d ./src/data-source.ts",
    "typeorm:migrate": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "gen-config-types": "NODE_OPTIONS='--import tsx' npx @rosen-bridge/config generate-ts-types --schema ./config/schema.json --output ./src/types/configs.ts --root-type RosenService2BaseConfig",
    "gen-config-defaults": "NODE_OPTIONS='--import tsx' npx @rosen-bridge/config generate-default --schema ./config/schema.json --output ./config/default.yml --format yaml"
  },
  "dependencies": {
    "@rosen-bridge/abstract-logger": "^2.0.1",
    "@rosen-bridge/bitcoin-esplora-scanner": "^3.0.1",
    "@rosen-bridge/bitcoin-observation-extractor": "^6.1.6",
    "@rosen-bridge/bitcoin-rpc-scanner": "^3.0.1",
    "@rosen-bridge/callback-logger": "^0.1.0",
    "@rosen-bridge/config": "^0.4.0",
    "@rosen-bridge/discord-notification": "^0.1.3",
    "@rosen-bridge/evm-observation-extractor": "^5.1.6",
    "@rosen-bridge/evm-rpc-scanner": "^4.0.2",
    "@rosen-bridge/extended-typeorm": "^0.2.1",
    "@rosen-bridge/health-check": "^7.0.1",
    "@rosen-bridge/json-bigint": "^0.1.0",
    "@rosen-bridge/log-level-check": "^2.0.1",
    "@rosen-bridge/observation-extractor": "^7.1.4",
    "@rosen-bridge/scanner": "8.0.1",
    "@rosen-bridge/scanner-interfaces": "0.1.0",
    "@rosen-bridge/scanner-sync-check": "^7.0.0",
    "@rosen-bridge/service-manager": "^0.3.0",
    "@rosen-bridge/tokens": "^3.1.1",
    "@rosen-bridge/watcher-data-extractor": "^12.1.3",
    "@rosen-bridge/winston-logger": "^1.0.3",
    "@rosen-ui/asset-calculator": "^2.1.8",
    "@rosen-ui/constants": "^0.4.1",
    "config": "^3.3.7",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/node": "^20.11.9",
    "@vitest/coverage-istanbul": "^2.1.9",
    "typescript": "^5.3.3",
    "vitest": "^2.1.9"
  }
}

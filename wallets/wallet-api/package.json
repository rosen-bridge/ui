{
  "name": "@rosen-ui/wallet-api",
  "version": "3.0.5",
  "private": true,
  "description": "This is a private package utilized within Rosen Bridge UI app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint --fix . && npm run prettify",
    "lint:check": "eslint . && npm run prettify:check",
    "prettify": "prettier --write . --ignore-path ../../.gitignore",
    "prettify:check": "prettier --check . --ignore-path ../../.gitignore",
    "release": "npm run build && npm publish --access public",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@rosen-bridge/tokens": "^4.0.1",
    "@rosen-network/base": "^0.5.1",
    "@rosen-ui/types": "^0.4.0"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": "11.6.2"
  }
}

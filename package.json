{
  "name": "@rosen-bridge/ui",
  "description": "ui kit for Rosen projects",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-bridge/ui.git"
  },
  "license": "GPL-3.0",
  "author": "Rosen Team",
  "scripts": {
    "prettify": "prettier --write . --ignore-path .gitignore",
